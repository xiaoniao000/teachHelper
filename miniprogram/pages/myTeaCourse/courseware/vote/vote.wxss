.page {
  background-color: whitesmoke;
  width: 100%;
  height: 100vh;
}

.blank {
  height: 35px;
}

.blank1 {
  height: 6px;
}

.input {
  margin: 10px auto 0 auto;
  width: 75%;
  background-color: white;
  border: 1px solid #bbb;
  position: relative;
  height: 90px;
  padding: 5px;
  font-size: 14px;
}

.input1 {
  background-color: white;
  width: 100%;
  height: 30px;
  border: 1px solid #bbb;
  padding: 0 3px;
